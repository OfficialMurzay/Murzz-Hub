-- Load Rayfield
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- Create the Window
local Window = Rayfield:CreateWindow({
    Name = "Murzz Hub | AFSE",
    LoadingTitle = "Murzz Hub | AFSE",
    LoadingSubtitle = "by You",
    ConfigurationSaving = { Enabled = false },
    KeySystem = false
})

-- Home Tab
local HomeTab = Window:CreateTab("Home", 4483362458)

-- Each stat in its own section

-- Durability
local DurabilitySection = HomeTab:CreateSection("Durability")
HomeTab:CreateLabel("0")

-- Agility
local AgilitySection = HomeTab:CreateSection("Agility")
HomeTab:CreateLabel("0")

-- Speed
local SpeedSection = HomeTab:CreateSection("Speed")
HomeTab:CreateLabel("0")

-- Sword
local SwordSection = HomeTab:CreateSection("Sword")
HomeTab:CreateLabel("0")

-- Strength
local StrengthSection = HomeTab:CreateSection("Strength")
HomeTab:CreateLabel("0")

-- Chakra
local ChakraSection = HomeTab:CreateSection("Chakra")
HomeTab:CreateLabel("0")

Rayfield:LoadConfiguration()
